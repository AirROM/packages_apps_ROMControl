<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:aokp="http://schemas.android.com/apk/res/com.aokp.romcontrol">

	<PreferenceCategory
		android:key="air"
		android:title="@string/air">

		<Preference
			android:key="air_website"
			android:title="@string/air_website_title"
			android:summary="@string/air_website_summary" />

		<Preference
			android:key="air_source"
			android:title="@string/air_source_title"
			android:summary="@string/air_source_summary" />

		<Preference
			android:key="air_openwise"
			android:title="@string/air_openwise_title"
			android:summary="@string/air_openwise_summary" />

	</PreferenceCategory>

	<PreferenceCategory
		android:key="devs"
		android:title="@string/devs">
		<com.aokp.romcontrol.widgets.DeveloperPreference
			android:title="Rdata"
			aokp:nameDev="r_data"
			aokp:twitterHandle="ko_ma"
			aokp:donateLink="@string/donate_rdata" />

		<com.aokp.romcontrol.widgets.DeveloperPreference
			android:title="Toktak"
			aokp:nameDev="TOKTAK"
			aokp:twitterHandle=""
			aokp:donateLink="@string/donate_toktak" />
		
		<com.aokp.romcontrol.widgets.DeveloperPreference
			android:title="Iodes"
			aokp:nameDev="IODES"
			aokp:twitterHandle=""
			aokp:donateLink="@string/donate_iodes" />

	</PreferenceCategory>

</PreferenceScreen>
